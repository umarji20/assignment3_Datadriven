import jxl.*;
import java.io.*;







def groovyUtils = new com.eviware.soapui.support.GroovyUtils(context)
def projectDir = groovyUtils.projectPath
log.info("projectdir" +projectDir)





def testData_file = projectDir+"\\data\\datasheet.xls";


Workbook wrkbk_TD = Workbook.getWorkbook(new File(testData_file))
Sheet sheet_TD = wrkbk_TD.getSheet(0)
int row_Count_Used_TD = sheet_TD.getRows()
int col_Count_Used_TD = sheet_TD.getColumns()

log.info("row"+row_Count_Used_TD)
log.info("col"+col_Count_Used_TD)

//def current_row = testCase.getPropertyValue("current_row")
def current_row =  testRunner.testCase.testSuite.project.getPropertyValue("Project_TestData_Row")
log.info(current_row)
if(Integer.parseInt(current_row)&lt;row_Count_Used_TD){
    for(int i = Integer.parseInt(current_row) ; i&lt;row_Count_Used_TD; i++){
        if( sheet_TD.getCell(1,Integer.parseInt(current_row)).getContents() == "1"){  
            for( int j = 2;j&lt;col_Count_Used_TD;j++){
                
                String data_Header = sheet_TD.getCell(j,0).getContents()    
                String data = sheet_TD.getCell(j,Integer.parseInt(current_row)).getContents()
                log.info(data_Header+" "+ data+" "+j +" " +col_Count_Used_TD)
               testRunner.testCase.setPropertyValue(data_Header, data)
            }
            int count = Integer.parseInt(current_row)+1
            testRunner.testCase.testSuite.project.setPropertyValue("Project_TestData_Row",count.toString())
            testRunner.testCase.setPropertyValue("current_row",count.toString())
            break;
        }
        current_row = String.valueOf(Integer.parseInt(current_row) +1)
    }
    
}
else{
log.info("Data is finished")
}</script></con:config></con:testStep><con:testStep type="restrequest" name="Client_Credentials" id="0efa33eb-8aaa-4d51-8b1b-aac3f42d3dd5"><con:settings/><con:config service="https://sit.apihub.citi.com" resourcePath="/clientCredentials/oauth2/token/bh/gcb" methodName="Gcb" xsi:type="con:RestRequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:restRequest name="Client_Credentials" id="4793256c-1779-497a-b45c-e8f079b0beb4" mediaType="application/x-www-form-urlencoded" postQueryString="false"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con:settings><con:endpoint>${#Project#ServiceEndPoint-APIM}</con:endpoint><con:request>${#TestCase#payload}</con:request><con:originalUri>https://aspac.sit.api.citi.com/gcbap/api/clientCredentials/oauth2/token/au/gcb</con:originalUri><con:assertion type="GroovyScriptAssertion" id="653cbeb6-6cc6-410b-af3b-6f924e4f35ed" name="Client_Credentials"><con:configuration><scriptText>import groovy.json.JsonSlurper
